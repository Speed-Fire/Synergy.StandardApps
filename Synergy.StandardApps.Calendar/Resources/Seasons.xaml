    <DrawingBrush x:Key="SummerBrush" Stretch="None">
        <DrawingBrush.Drawing>

            <DrawingGroup>

                <GeometryDrawing>

                    <GeometryDrawing.Brush>
                        <RadialGradientBrush GradientOrigin="0.5, 0.5">
                            <GradientStop Color="Yellow" Offset="0"/>
                            <GradientStop Color="LightYellow" Offset="0.5"/>
                            <GradientStop Color="Transparent" Offset="1"/>
                        </RadialGradientBrush>
                    </GeometryDrawing.Brush>

                    <GeometryDrawing.Geometry>
                        <EllipseGeometry Center="0, 0" RadiusX="225" RadiusY="225"/>
                    </GeometryDrawing.Geometry>

                </GeometryDrawing>

                <GeometryDrawing>

                    <GeometryDrawing.Brush>
                        <RadialGradientBrush GradientOrigin="0.5 0.5">
                            <GradientStop Color="Orange" Offset="0"/>
                            <GradientStop Color="Orange" Offset="0.8"/>
                            <GradientStop Color="Yellow" Offset="1"/>
                        </RadialGradientBrush>
                    </GeometryDrawing.Brush>
                    
                    <GeometryDrawing.Geometry>
                        <EllipseGeometry Center="0, 0" RadiusX="75" RadiusY="75"/>
                    </GeometryDrawing.Geometry>

                </GeometryDrawing>
                
            </DrawingGroup>
            
        </DrawingBrush.Drawing>

        <DrawingBrush.RelativeTransform>
            <TranslateTransform X="-0.475" Y="-0.45"/>
        </DrawingBrush.RelativeTransform>
        
    </DrawingBrush>

